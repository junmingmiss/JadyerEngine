<!-- Logback -->
<!-- http://feitianbenyue.iteye.com/blog/2206273 -->
<!-- ch.qos.logback.ext.spring.web.LogbackConfigListener -->
<listener>
	<listener-class>com.jadyer.engine.common.base.LogbackConfigListener</listener-class>
</listener>
<context-param>
	<param-name>logbackConfigLocation</param-name>
	<param-value>/WEB-INF/log/logback-${appenv.active}.xml</param-value>
</context-param>